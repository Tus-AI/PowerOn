TARGET = ACCOUNT

DEFINE
X=NUMBER
I=NUMBER
SHTYPE=NUMBER ARRAY(999)
BL=NUMBER ARRAY(999)
END

SELECT
ACCOUNT:USERCHAR3="05-PORTALLIANCE" AND ACCOUNT:CLOSEDATE='--/--/--'
END

PRINT TITLE="PORT ALL"
X=0

FOR EACH SHARE WITH SHARE:SHARECODE=1 
 DO
  X=X+1
  SHTYPE(X)=SHARE:TYPE
  BL(X)=SHARE:BALANCE
 END
 

  FOR EACH SHARE WITH (SHARE:TYPE=00 AND SHARE:CLOSEDATE='--/--/--' AND SHARE:DIVTYPE=100) 
   DO
     FOR I=0 to X
	 DO
     PRINT ACCOUNT:NUMBER
     PRINT "|"
     PRINT ACCOUNT:TYPE
     print "|"
     PRINT ACCOUNT:OPENDATE
     PRINT "|"
     PRINT NAME:BIRTHDATE
     PRINT "|"
     PRINT SHARE:BALANCE
	 PRINT "|"
	 PRINT SHTYPE(X)
	 PRINT "|"
	 PRINT BL(X)
	 NEWLINE
	END
 END

END
